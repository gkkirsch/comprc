# export GIT_MERGE_AUTOEDIT=no # don't open vim when pulling master

gmm(){
  git checkout master
  git pull origin master
  git checkout -
  git merge master
  git push --no-verify
}

alias uuid="uuidgen | tr \"[:upper:]\" \"[:lower:]\" | tr -d '[:space:]' | pbcopy && pbpaste"

# Enable iex history
export ERL_AFLAGS="-kernel shell_history enabled"
